{
    "name": "VisualLingo",
    "description": "An extension for Google Meet to translate the Filipino Sign Language to help the Filipino mute community.",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [{
        "matches": [
            "https://meet.google.com/*"
        ],
        "js": [
            "src/scripts/tf.min.js",
            "src/scripts/teachablemachine-pose.min.js",
            "src/scripts/core.js"
        ]
    }],
      "permissions": [
        "tabs",
        "storage"
      ],
      "icons": {
            "16": "icons/Visuallingo.png",
            "48": "icons/Visuallingo.png",
            "128": "icons/Visuallingo.png"
      },
      "action": {
        "default_popup": "src/popup/popup.html",
        "default_icon": {
            "16": "icons/Visuallingo.png",
            "48": "icons/Visuallingo.png",
            "128": "icons/Visuallingo.png"
          }
      },
      "background": {
        "service_worker": "src/scripts/background.js"
      }
}
    